#!groovy
pipeline{
 agent any
 
  tools {
    maven 'maven'
  }
 
 stages{
 
   stage("build"){
   
       steps{
       
          bat "mvn -f ./appointmentdocker clean install -DskipTests"
          bat "docker build -f appointmentdocker/dockerfile -t appointmentdockerjenkinsapp ./appointmentdocker"
         
       }
   
   }
 
 }
 
 
 
}
